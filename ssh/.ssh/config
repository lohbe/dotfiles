Host zeus
  HostName zeus.lan
  User ben
  IdentityFile ~/.ssh/id_ecdsa

Host the-dam.org
  HostName the-dam.org
  User ben
  IdentityFile ~/.ssh/id_ecdsa

Host localhost 172.* 192.* 10.* *.lan *.isilon.* *.dellcsc.*
  SetEnv TERM=xterm-256color
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel Quiet

Host laptop
  HostName 192.168.192.235
  User ben
